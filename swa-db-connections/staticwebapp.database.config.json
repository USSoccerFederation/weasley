{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v0.9.7/dab.draft.schema.json",
  "data-source": {
    "database-type": "cosmosdb_nosql",
    "connection-string": "AccountEndpoint=https://usoc-db-dev.documents.azure.com:443/;AccountKey=1VX5Uu4b44JyoL2joHnSLuQP76UQxUjsFpeNxGrPM4RkoDfRBqUUMClCl4dXWkaPkczLwupg5076ACDbwq1LpQ==;",
    "options": {
      "database": "Users",
